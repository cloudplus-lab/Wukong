{
	"metadata":{
		"name":"createinstancetemplaterequest",
		"namespace":"jdvm"
	},
	"apiVersion":"cloudplus.io/v1alpha3",
	"kind":"JDVM",
	"spec":{
		"lifecycle":{
			"createInstanceTemplateRequest":{
				"regionId":"String",
				"name":"String",
				"description":"String",
				"instanceTemplateData":{
					"elasticIp":{
						"chargeMode":"String",
						"provider":"String",
						"bandwidthMbps":1
					},
					"password":"String",
					"imageId":"String",
					"systemDisk":{
						"noDevice":true,
						"cloudDiskSpec":{
							"snapshotId":"String",
							"diskType":"String",
							"diskSizeGB":1
						},
						"autoDelete":true,
						"diskCategory":"String",
						"deviceName":"String"
					},
					"instanceType":"String",
					"primaryNetworkInterface":{
						"networkInterface":{
							"subnetId":"String",
							"sanityCheck":1
						},
						"autoDelete":true,
						"deviceIndex":1
					}
				}
			}
		}
	}
}